{
  "name": "zod-to-camel-case",
  "description": "Convert zod schema object keys to camel case",
  "version": "0.4.5",
  "license": "MIT",
  "main": "./dist/npm/index.js",
  "types": "./dist/npm/types.d.ts",
  "type": "module",
  "scripts": {
    "check-types": "tsc --noEmit",
    "lint": "npx prettier . --check",
    "test": "vitest run --coverage",
    "test:watch": "vitest run --watch",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-report.junit.xml",
    "lint:format": "npx prettier . --write",
    "build": "npm run build:npm",
    "build:npm": "rollup -c rollup.config.ts --configPlugin typescript"
  },
  "exports": {
    ".": "./dist/npm/index.js"
  },
  "files": [
    "./dist/npm",
    "./package.json",
    "./README.md"
  ],
  "devDependencies": {
    "@codecov/rollup-plugin": "^1.9.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-virtual": "^3.0.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/yargs": "^17.0.34",
    "@vitest/coverage-v8": "^4.0.18",
    "prettier": "^3.8.1",
    "rollup": "^4.52.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "tsx": "^4.17.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.x"
  },
  "peerDependencies": {
    "zod": "^4.3.6"
  }
}
